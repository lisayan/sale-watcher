{
    "manifest_version": 2,
    "name": "Sale Watcher",
    "version": "0.1",
    "permissions": [
        "bookmarks"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "key": "",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["jquery-3.4.1.min.js","content.js"]
        }
    ]
}

